<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>fs</servlet-name>
		<servlet-class>org.kotemaru.gae.simplefs.SimpleFsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fs</servlet-name>
		<url-pattern>/fs/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>user</servlet-name>
		<servlet-class>org.kotemaru.gae.simplefs.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>user</servlet-name>
		<url-pattern>/user/*</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>score</servlet-name>
		<servlet-class>org.kotemaru.gae.simplefs.ScoreServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>score</servlet-name>
		<url-pattern>/score/*</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
